{
  "name": "vue3-firebase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && VITE_TIMESTAMP_HASH='$(date -Iseconds)-$(git rev-parse --short HEAD)' vite build",
    "preview": "vite preview",
    "test": "vitest --bail 0 --run --passWithNoTests",
    "lint": "eslint --ext .ts,.vue src",
    "typecheck": "vue-tsc",
    "emulate": "firebase emulators:start --import firebaseEmulatorData --export-on-exit",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.1",
    "@phosphor-icons/vue": "^2.1.6",
    "@tiptap/extension-bullet-list": "^2.2.2",
    "@tiptap/extension-highlight": "^2.2.2",
    "@tiptap/extension-list-item": "^2.2.2",
    "@tiptap/extension-ordered-list": "^2.2.2",
    "@tiptap/extension-task-item": "^2.2.2",
    "@tiptap/extension-task-list": "^2.2.2",
    "@tiptap/pm": "^2.2.2",
    "@tiptap/starter-kit": "^2.2.2",
    "@tiptap/vue-3": "^2.2.2",
    "axios": "^1.6.7",
    "firebase": "^10.8.0",
    "pinia": "^2.1.7",
    "vee-validate": "^4.12.5",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/vue": "^8.0.2",
    "@types/jsdom": "^21.1.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.0.10",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  }
}
