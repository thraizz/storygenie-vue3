{
  "name": "vue3-firebase",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:emulate\" \"npm run emulate\"",
    "dev:emulate": "VITE_USE_EMULATORS=true vite",
    "build": "vue-tsc && VITE_APP_VERSION=\"$npm_package_version\" VITE_TIMESTAMP_HASH=\"$(date -Iseconds)-$(git rev-parse --short HEAD)\" vite build",
    "preview": "vite preview",
    "test": "vitest --bail 0 --run --passWithNoTests",
    "lint": "eslint --ext .ts,.vue src",
    "typecheck": "vue-tsc",
    "emulate": "npx firebase emulators:start --import firebaseEmulatorData",
    "emulate:export": "npx firebase emulators:start --import firebaseEmulatorData --export-on-exit",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@phosphor-icons/vue": "^2.2.1",
    "@sentry/vite-plugin": "^3.3.1",
    "@sentry/vue": "^9.14.0",
    "@tiptap/extension-bullet-list": "^2.11.7",
    "@tiptap/extension-highlight": "^2.11.7",
    "@tiptap/extension-list-item": "^2.11.7",
    "@tiptap/extension-ordered-list": "^2.11.7",
    "@tiptap/extension-task-item": "^2.11.7",
    "@tiptap/extension-task-list": "^2.11.7",
    "@tiptap/pm": "^2.11.7",
    "@tiptap/starter-kit": "^2.11.7",
    "@tiptap/vue-3": "^2.11.7",
    "axios": "^1.9.0",
    "clipboard-polyfill": "^4.1.1",
    "concurrently": "^9.1.2",
    "date-fns": "^4.1.0",
    "firebase": "^10.14.1",
    "pinia": "^2.3.1",
    "radix-vue": "^1.9.17",
    "vee-validate": "^4.15.0",
    "vue": "^3.4.29",
    "vue-router": "^4.5.0",
    "vuefire": "^3.2.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "jsdom": "^23.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.87.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^5.4.18",
    "vitest": "^1.6.1",
    "vue-tsc": "^2.2.10"
  }
}
